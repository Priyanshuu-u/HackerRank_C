#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char ch;
    char str[20];
    char string[100];
    scanf("%c",&ch);
    scanf("%s",str);
    scanf("\n");
    scanf("%[^\n]%*c",string);
    printf("%c \n",ch);
    printf("%s \n",str);
    printf("%s",string);
        

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
